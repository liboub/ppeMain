{# empty Twig template #}


    <table id="tableau" class="table table-striped table-bordered" width="100%" cellspacing="0">
        <thead>
            <tr>
                {% for i in 0..entete|length-1 %}
                    <th>{{entete[i]}}</th>
                {% endfor %}
            </tr>
        </thead> 
        
        <tbody>
    
            {% for ligne in donneesTableau %}
                <tr>
                    {% for i in 1..entete|length %}
                       <td><a href="{{href}}{{ligne[0]}}#{{idZone}}">{{ligne[i]}}</a></td>
                    {% endfor %}
                </tr>    
            {% endfor %}
            
        </tbody>
    </table>
    

  