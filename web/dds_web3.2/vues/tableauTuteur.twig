{# empty Twig template #}

    <table id="tableau" class="table table-striped table-bordered" width="100%" cellspacing="0">
        <thead>
            <tr>
                {% for i in 0..enteteT|length-1 %}
                    <th>{{enteteT[i]}}</th>
                {% endfor %}
            </tr>
        </thead> 
        
        <tbody>
    
            {% for ligne in donneesTableauT %}
                <tr>
                    {% for i in 1..enteteT|length %}
                       <td><a href="{{hrefT}}{{ligne[0]}}#{{idZoneT}}">{{ligne[i]}}</a></td>
                    {% endfor %}
                </tr>    
            {% endfor %}
            
        </tbody>
    </table>
    
